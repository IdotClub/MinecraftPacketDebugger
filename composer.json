    {
      "type": "vcs",
      "url": "https://github.com/pmmp/SPL"
    },
    {
      "type": "vcs",
      "url": "https://github.com/pmmp/BinaryUtils"
    },
    {
      "type": "vcs",
      "url": "https://github.com/pmmp/NBT"
    },
    {
      "type": "vcs",
      "url": "https://github.com/pmmp/Math"
    },
    {
      "type": "vcs",
      "url": "https://github.com/pmmp/Snooze"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
