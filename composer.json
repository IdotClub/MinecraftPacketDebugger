{
  "name": "jacknoordhuis/minecraft-packet-debugger",
  "description": "Tool for debuging minecraft bedrock network communication.",
  "type": "project",
  "authors": [
    {
      "name": "Jack Noordhuis",
      "email": "daswaggamer@gmail.com"
    }
  ],
  "require": {
    "pocketmine/pocketmine-mp": "3.19.1",
    "nxtlvlsoftware/static-constructors": "dev-master",
    "symfony/console": "^4.2"
